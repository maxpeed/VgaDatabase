{
  "name": "vgait/vgadatabase",
  "description": "This is a database abstraction for easy instantiation and usage of PDO",
  "require": {
    "php": ">=7.1.0",
    "ext-pdo": "*",
    "vgait/vga-exception": "^1.0"
  },
  "autoload": {
    "psr-4": {
      "VgaDatabase\\": "src/"
    }
  },
  "autoload-dev": {
    "VgaDatabase\\Tests": "tests/"
  },
  "require-dev": {
    "phpunit/phpunit": "^6.0"
  }
}
